var optimizelyThirdParty = {"dataProviders": [{"functions": {
  fetchData: function() {
    var CAMPAIGN_ID_PARAM_NAME = 'opti_ca';
    var AD_GROUP_ID_PARAM_NAME = 'opti_ag';
    var OPTIMIZELY_KEYWORD = 'opti_key';
    var KEYWORD_PREFIX = 'kwd-';
    var VALUE_DELIMITER = ':';
    var data = {};
    var params = {};

    var queryParamPairs = window.location.search.substr(1).split('&');
    var i, pair, ids;

    for (i = 0; i < queryParamPairs.length; i++) {
      pair = queryParamPairs[i].split('=');
      params[pair[0]] = pair[1];
    }

    if (params[CAMPAIGN_ID_PARAM_NAME] && params[AD_GROUP_ID_PARAM_NAME]) {
      // All IDs shall already be URI component encoded.
      // Adwords is the only ad integration in which we do NOT look for opti_source
      // due to it being the first.
      ids = [params[CAMPAIGN_ID_PARAM_NAME], params[AD_GROUP_ID_PARAM_NAME]];
      data['campaign'] = ids[0];
      data['adgroup'] = ids.slice(0, 2).join(VALUE_DELIMITER);
      window['optimizely'].push(['storeThirdPartyData', 'google_adwords', data, true]);
    }

    if (params[OPTIMIZELY_KEYWORD] && params [AD_GROUP_ID_PARAM_NAME]) {
      // The value of this param is {targetid}, which for keywords prefixes them
      // with 'kwd-', so that needs to be stripped. Note: there should only
      // be one keyword, not multiples.
      var re = /(kwd\-(\d+))[\:\&]?/g, match = null, keywords = [];
      while ((match = re.exec(params[OPTIMIZELY_KEYWORD])) !== null) {
          keywords.push(match[2]);
      }
      var adGroupId = params[AD_GROUP_ID_PARAM_NAME];
      var keywordData = {
        'keywords': [adGroupId, keywords[0]].join(VALUE_DELIMITER),
      };
      window['optimizely'].push(['storeThirdPartyData', 'google_adwords', keywordData, true]);
    }
  }
}
